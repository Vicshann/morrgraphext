	<compound name="action">
		<add name="Action Name" type="string" ver2="16909312" />
		<add name="Action File Name" type="string" />
		<add name="Unknown Int 1" type="int" />
		<add name="Num Events" type="int" />
		<add name="Events?" type="events?" arr1="Num Events" />
		<add name="Unknown Int 2" type="int" />
	</compound>
	<compound name="event string">
		<add name="Unknown Int" type="int" />
